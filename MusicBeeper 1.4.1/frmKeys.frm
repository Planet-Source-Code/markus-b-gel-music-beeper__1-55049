Attribute VB_Name = "frmKeys"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

'properties for moving the form
Private m_bDragging As Boolean 'flag: window is moved
Private m_lDownX As Long 'point on which the mouse was
Private m_lDownY As Long 'put down over the form

Public m_iTop As Integer 'top border of g_frmMusicBeeper
Public m_iBottom As Integer 'bottom border of g_frmMusicBeeper
Public m_iLeft As Integer  'left border of g_frmMusicBeeper
Public m_iRight As Integer  'right border of g_frmMusicBeeper

Public m_iFocus As Integer 'flag which indicates if the focus has changed to another control

' ##################################################################################
'
' form events
'
' ##################################################################################

Private Sub Form_Load()
Dim i As Integer

  For i = 47 To 53
    cmdKey(i).Enabled = False
  Next i
  
  SetCaptions
  SetBorder Me, EB_SIMPLE

  Top = g_frmMusicBeeper.Top - Height
  Left = g_frmMusicBeeper.Left
  g_eKeyDocked = ED_BOTTOM
  m_iFocus = 0

End Sub

Private Sub Form_Activate()
  
  m_iFocus = 0
  
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
  
  g_clsMusicBeeper.GetKey KeyCode, Shift
  
End Sub

Private Sub Form_MouseDown(Button As Integer, Shift As Integer, _
                           x As Single, Y As Single)
  
  If (x < 106 Or x > Width - 106 _
  Or Y < 106 Or Y > Height - 106) Then  'border
    m_lDownX = x
    m_lDownY = Y
    m_bDragging = True
  End If

End Sub

Private Sub Form_MouseMove(Button As Integer, Shift As Integer, _
                           x As Single, Y As Single)
  
  If m_bDragging Then
    'pull window
    Move Left + (x - m_lDownX), Top + (Y - m_lDownY)
  End If
  
End Sub

Private Sub Form_MouseUp(Button As Integer, Shift As Integer, _
                         x As Single, Y As Single)
  m_bDragging = False
  FormMove Me, g_eKeyDocked
  
  Select Case g_eKeyDocked
  Case ED_TOP
    Top = m_iTop - Height
  Case ED_LEFT
    Left = m_iLeft - Width
  Case ED_BOTTOM
    Top = m_iBottom
  Case ED_RIGHT
    Left = m_iRight
  Case ED_NO
    'no action
  End Select

End Sub

' ##################################################################################
'
' control events
'
' ##################################################################################

Private Sub cmdKey_GotFocus(Index As Integer)
  
  If m_iFocus > 0 And Index < 47 Then
    g_clsMusicBeeper.GenerateBeep g_clsMusicBeeper.KeyCodes(Index), g_lDuration
    m_iFocus = 1
  Else
    m_iFocus = 2 'happens only when form is activated
                  'or when key is triggered from g_frmMusicBeeper
  End If

End Sub

Private Sub cmdKey_Click(Index As Integer)

  If m_iFocus = 2 And Index < 47 Then
    g_clsMusicBeeper.GenerateBeep g_clsMusicBeeper.KeyCodes(Index), g_lDuration
  End If
  m_iFocus = 2

End Sub

